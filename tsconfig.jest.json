{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"lib": [
			"DOM",
			"ES2020"
		],
		"baseUrl": "./src",
		"paths": {
			"store/*": [
				"store/*"
			],
			"hooks/*": [
				"hooks/*"
			],
			"components/*": [
				"components/*"
			],
			"pages/*": [
				"pages/*"
			],
			"router/*": [
				"router/*"
			],
			"types/*": [
				"types/*"
			]
		},
		"moduleResolution": "node",
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"types": [
			"jest",
			"@testing-library/jest-dom",
			"@testing-library/react"
		]
	},
	"include": [
		"src"
	]
}